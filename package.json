{
  "name": "chatters-server",
  "version": "1.0.0",
  "scripts": {
    "dev": "node --env-file=.env -r ts-node/register --watch src/index.ts",
    "start": "node dist/src/index.js",
    "build": "rm -rf dist && tsc",
    "migrations-up:dev": "node --env-file=.env -r ts-node/register src/migrations/execute.ts up",
    "migrations-down:dev": "node --env-file=.env -r ts-node/register src/migrations/execute.ts down",
    "migrations-up": "node dist/src/migrations/execute.js up",
    "migrations-down": "node dist/src/migrations/execute.js down"
  },
  "dependencies": {
    "@fastify/cors": "^8.3.0",
    "@fastify/swagger": "^8.10.1",
    "@fastify/swagger-ui": "^1.9.3",
    "@fastify/websocket": "^8.1.0",
    "@sinclair/typebox": "^0.25.24",
    "fastify": "^4.19.2",
    "jsonwebtoken": "^9.0.1",
    "kysely": "^0.23.5",
    "pg": "^8.11.1"
  },
  "devDependencies": {
    "@fastify/type-provider-typebox": "^2.4.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.1",
    "@types/pg": "^8.10.2",
    "@types/ws": "^8.5.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
